<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
	Author : Genesys Framework 
	Description: Purpose 
		of the document follows. 
-->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Products" table="PRODUCTS">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="CODE" name="code" />
		<property column="IS_MAIN" name="isMain" type="int" />
		<property column="NAME" name="name" />
		<property column="PRICE" name="price" type="int" />
		<property column="PLEIXING_ID" name="pleixingId" type="int" />
		<property column="COMMENT" name="comment" />
		<property column="WEBURL" name="weburl" />
		<property column="SALE_ADDR" name="saleAddr" />
		<property column="STATUS" name="status" type="int" />
		<property column="UP_TIME" name="upTime" type="timestamp"/>
		<property column="DOWN_TIME" name="downTime" type="timestamp"/>
		<property column="IMG_PATH1" name="imgPath1" />
		<property column="IMG_PATH2" name="imgPath2" />
		<property column="IMG_PATH3" name="imgPath3" />
		<property column="IMG_PATH4" name="imgPath4" />
		<property column="IMG_PATH5" name="imgPath5" />
		<property column="ACCOUNT_ID" name="accountId" type="int" />
		<property column="WRITE_TIME" name="writeTime" type="timestamp"/>

	</class>
</hibernate-mapping>