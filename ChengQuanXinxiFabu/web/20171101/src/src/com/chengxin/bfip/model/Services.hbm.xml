<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
	Author : Genesys Framework 
	Description: Purpose of the document follows. 
-->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Services" table="SERVICES">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="CODE" name="code" />
		<property column="ACCOUNT_ID" name="accountId" type="int"/>
		<property column="FENLEI_ID" name="fenleiId" type="int"/>
		<property column="NAME" name="name" />
		<property column="CITY_ID" name="cityId" type="int"/>
		<property column="COMMENT" name="comment" />
		<property column="WEBURL" name="weburl" type="text" />
		<property column="ADDR" name="addr" type="text" />
		<property column="IS_SHOW" name="isShow" type="int"/>
		<property column="STATUS" name="status" type="int"/>
		<property column="UP_TIME" name="upTime" type="timestamp"/>
		<property column="DOWN_TIME" name="downTime" type="timestamp"/>
		<property column="CONTACT_NAME" name="contactName" />
		<property column="CONTACT_MOBILE" name="contactMobile" />
		<property column="CONTACT_WEIXIN" name="contactWeixin" />
		<property column="LOGO" name="logo" />
		<property column="IMG_PATH1" name="imgPath1" />
		<property column="IMG_PATH2" name="imgPath2" />
		<property column="IMG_PATH3" name="imgPath3" />
		<property column="IMG_PATH4" name="imgPath4" />
		<property column="IMG_PATH5" name="imgPath5" />
		<property column="WRITE_TIME" name="writeTime" type="timestamp"/>

	</class>
</hibernate-mapping>