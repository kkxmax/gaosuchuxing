<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="City" table="CITIES">
    <id column="ID" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="NAME" name="name"/>
    <property column="PROVINCE_ID" name="provinceId" type="int"/>
    
  </class>
</hibernate-mapping>