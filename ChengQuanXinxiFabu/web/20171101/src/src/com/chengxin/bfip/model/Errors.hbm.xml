<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
	Author : Genesys Framework 
	Description: Purpose 
		of the document follows. 
-->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Errors" table="ERRORS">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="BOOKNUM" name="booknum" />
		<property column="NAME" name="name" />
		<property column="NO_NAME" name="noName" />
		<property column="REAL_NAME" name="realName" />
		<property column="CONTENT" name="content" />
		<property column="REASON" name="reason" />
		<property column="WHYIS" name="whyis" />
		<property column="UPDATE_TIME" name="updateTime" type="timestamp"/>
		<property column="STATUS" name="status" type="int" />
		<property column="ACCOUNT_ID" name="accountId" type="int" />
		<property column="WRITE_TIME" name="writeTime" type="timestamp"/>

	</class>
</hibernate-mapping>