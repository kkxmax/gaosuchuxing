<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Author     : Genesys Framework
    Description:
        Purpose of the document follows.
-->
<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="Hots" table="HOTS">
    <id column="ID" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="TITLE" name="title"/>
    <property column="KIND" name="kind"/>
    <property column="VISIT_CNT" name="visitCnt" type="int"/>
    <property column="COMMENT_CNT" name="commentCnt" type="int"/>
    <property column="REMARK_CNT" name="remarkCnt" type="int"/>
    <property column="SHARE_CNT" name="shareCnt" type="int"/>
    <property column="UP_TIME" name="upTime" type="timestamp"/>
    <property column="DOWN_TIME" name="downTime" type="timestamp"/>
    <property column="CONTENT" name="content"/>
    <property column="ST" name="st"/>
    <property column="ACCOUNT_ID" name="accountId"/>
    <property column="WRITE_TIME" name="writeTime" type="timestamp"/>
    
  </class>
</hibernate-mapping>