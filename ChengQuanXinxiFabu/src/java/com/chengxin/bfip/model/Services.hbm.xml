<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Document : Member.hbm.xml Author : Genesys Framework Description: Purpose 
	of the document follows. -->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Services" table="SERVICES">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="NUM" name="num" />
		<property column="BOOKNUM" name="booknum" />
		<property column="NAME" name="name" />
		<property column="COMMENT" name="comment" />
		<property column="WEBURL" name="weburl" type="text" />
		<property column="ADDR" name="addr" type="text" />
		<property column="UP_TIME" name="up_time" />
		<property column="DOWN_TIME" name="down_time" />
		<property column="CONTACT_NAME" name="contact_name" />
		<property column="CONTACT_MOBILE" name="contact_mobile" />
		<property column="CONTACT_WEIXIN" name="contact_weixin" />
		<property column="ISSUE" name="issue" />
		<property column="BOOKTYPE" name="booktype" type="int" />
		<property column="ST" name="st" type="int" />
		<property column="IMG_PATH1" name="img_path1" />
		<property column="IMG_PATH2" name="img_path2" />
		<property column="IMG_PATH3" name="img_path3" />
		<property column="IMG_PATH4" name="img_path4" />
		<property column="IMG_PATH5" name="img_path5" />

	</class>
</hibernate-mapping>