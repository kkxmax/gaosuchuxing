<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Document : Member.hbm.xml Author : Genesys Framework Description: Purpose 
	of the document follows. -->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Errors" table="ERRORS">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="BOOKNUM" name="booknum" />
		<property column="NAME" name="name" />
		<property column="NO_NAME" name="no_name" />
		<property column="REAL_NAME" name="real_name" />
		<property column="CONTENT" name="content" />
		<property column="REASON" name="reason" />
		<property column="WHYIS" name="whyis" />
		<property column="UPDATE_TIME" name="update_time" />
		<property column="STATUS" name="status" type="int" />
		<property column="ACCOUNT_ID" name="account_id" type="int" />
		<property column="WRITE_TIME" name="write_time" />

	</class>
</hibernate-mapping>