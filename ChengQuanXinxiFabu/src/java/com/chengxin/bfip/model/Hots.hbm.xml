<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Document   : Member.hbm.xml
    Author     : Genesys Framework
    Description:
        Purpose of the document follows.
-->
<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="Hots" table="HOTS">
    <id column="ID" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="TITLE" name="title"/>
    <property column="KIND" name="kind"/>
    <property column="VISIT_CNT" name="visit_cnt" type="int"/>
    <property column="COMMENT_CNT" name="comment_cnt" type="int"/>
    <property column="REMARK_CNT" name="remark_cnt" type="int"/>
    <property column="SHARE_CNT" name="share_cnt" type="int"/>
    <property column="UP_TIME" name="up_time" type="date"/>
    <property column="DOWN_TIME" name="down_time" type="date"/>
    <property column="CONTENT" name="content"/>
    <property column="ST" name="st"/>
    <property column="ACCOUNT_ID" name="account_id"/>
    <property column="WRITE_TIME" name="write_time"/>
    
  </class>
</hibernate-mapping>