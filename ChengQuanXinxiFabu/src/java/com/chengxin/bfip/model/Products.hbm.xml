<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Document : Member.hbm.xml Author : Genesys Framework Description: Purpose 
	of the document follows. -->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Products" table="PRODUCTS">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="NUM" name="num" />
		<property column="BOOKNUM" name="booknum" />
		<property column="ENTER_NAME" name="enter_name" />
		<property column="STATUS" name="status" type="int" />
		<property column="NAME" name="name" />
		<property column="PRICE" name="price" type="int" />
		<property column="PLEIXING_ID" name="pleixing_id" type="int" />
		<property column="COMMENT" name="comment" />
		<property column="WEBURL" name="weburl" />
		<property column="SALE_WEBURL" name="sale_weburl" />
		<property column="UP_TIME" name="up_time" />
		<property column="DOWN_TIME" name="down_time" />
		<property column="IMG_PATH1" name="img_path1" />
		<property column="IMG_PATH2" name="img_path2" />
		<property column="IMG_PATH3" name="img_path3" />
		<property column="IMG_PATH4" name="img_path4" />
		<property column="IMG_PATH5" name="img_path5" />
		<property column="ACCOUNT_ID" name="account_id" type="int" />
		<property column="WRITE_TIME" name="write_time" />

	</class>
</hibernate-mapping>