<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="Pleixing" table="PLEIXINGS">
    <id column="ID" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="UPPER_ID" name="upper_id" type="int"/>
    <property column="TITLE" name="title"/>
    <property column="ACCOUNT_ID" name="account_id" type="int"/>
    <property column="WRITE_TIME" name="write_time" type="date"/>
    
  </class>
</hibernate-mapping>